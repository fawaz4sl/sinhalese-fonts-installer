<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="30"
	android:versionName="3.0" package="org.herrlado.fonts">

	<application android:label="@string/app_name"
		android:debuggable="false" android:icon="@drawable/fonts">
		<activity android:name="org.herrlado.sinhfonts.Installer" 
		android:label="@string/app_name"
		android:configChanges="orientation">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

	</application>
	<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="8"></uses-sdk>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
</manifest>